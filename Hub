local Button = MainTab:CreateButton({
    Name = "?",
    Callback = function() 
local Sound = Instance.new("Sound")
Sound.Name = "MyMusic"
Sound.SoundId = "rbxassetid://1837767240"
Sound.Volume = 5
Sound.Looped = true
Sound.Parent = workspace 

Sound:Play()
     end,
})

local MainSection = MainTab:CreateSection("Off music")

local Button = MainTab:CreateButton({
   Name = "script",
   Callback = function() 
for _, obj in pairs(workspace:GetChildren()) do
    if obj:IsA("Sound") and obj.Name == "HacklordTheme" then
        obj:Destroy()
    end
end
   end,
})

