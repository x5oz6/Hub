addButton(tab, "3", "Click To Run", 'https://rawscripts.net/raw/Universal-Script-free-gamepass-works-in-some-games-23863')
addButton(tab, "wyborn", "Click To Run", 'https://raw.githubusercontent.com/ckw69/Wyborn/main/wyborn')
addButton(tab, "wyvern", "Click To Run", 'https://raw.githubusercontent.com/wyvernleaker/WYVERN/main/WYVERN.lua')

tab = createTab("-•Premium•-", "ImageIdHere")

tab = createTab("-•W.I.P•-", "ImageIdHere")
