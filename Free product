-- Delete duplicate ScreenGui by name (keep the first instance)
local Players = game:GetService("Players")
local LP = Players.LocalPlayer
local StarterGui = game:GetService("StarterGui")

local function getHUI()
    if typeof(gethui) == "function" then
        local ok, gui = pcall(gethui); if ok and gui then return gui end
    end
    return nil
end

local CONTAINERS = {
    game:GetService("CoreGui"),
    LP:WaitForChild("PlayerGui"),
    getHUI()
}

local DRY_RUN = false -- true = chỉ log, false = xóa thật

local function notify(msg)
    pcall(function()
        StarterGui:SetCore("SendNotification", {Title="GUI Cleaner", Text=msg, Duration=4})
    end)
end

local function clean(parent)
    if not parent then return end
    local seen = {}  -- name -> first instance we keep
    for _, inst in ipairs(parent:GetChildren()) do
        if inst:IsA("ScreenGui") then
            local key = string.lower(inst.Name)
            if not seen[key] then
                seen[key] = inst
            else
                if DRY_RUN then
                    print(("[DRY] Would delete duplicate '%s' at %s"):format(inst.Name, inst:GetFullName()))
                else
                    print(("[DEL] Duplicate '%s' -> %s"):format(inst.Name, inst:GetFullName()))
                    pcall(function() inst:Destroy() end)
                end
            end
        end
    end
end

for _, parent in ipairs(CONTAINERS) do
    clean(parent)
end

notify("Đã dọn GUI trùng tên (giữ lại bản đầu).")

local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("03y_o - free product", "Default")

game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Thank you",
    Text = "wow",
    Duration = 0.5
})

local originalButtons = {}

local function addButton(tab, title, description, url)
    tab.newLabel(" ")
    local button = tab.newButton(title, description, function()
        loadstring(game:HttpGet(url))()
    end)
    table.insert(originalButtons, {tab = tab, title = title, description = description, url = url})
end

local function createTab(tabName, imageId)
    local tab = DrRayLibrary.newTab(tabName, imageId)

    tab.newButton("join now", "Discord me", function()
        setclipboard("")
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "Copied!",
            Text = "Link Has Been Copied From Your Clipboard.",
            Duration = 3
        })
    end)

    tab.newInput("Search", "Enter a keyword to search a game name.", function(inputSearch)
        search = inputSearch
    end)

    tab.newButton("Enter Search", "Searches For The Keyword", function()
        searchButtons(tab, search)
    end)

    tab.newButton("Refresh", "Refresh List's", function()
        refreshButtons(tab)
    end)

    return tab
end

local function refreshButtons(tab)
    for _, child in ipairs(tab:GetChildren()) do
        if child:IsA("GuiButton") then
            child:Destroy()
        end
    end
    for _, btn in ipairs(originalButtons) do
        if btn.tab == tab then
            addButton(tab, btn.title, btn.description, btn.url)
        end
    end
end

local function searchButtons(tab, keyword)
    for _, child in ipairs(tab:GetChildren()) do
        if child:IsA("GuiButton") then
            child:Destroy()
        end
    end
    for _, btn in ipairs(originalButtons) do
        if btn.tab == tab and string.find(btn.title:lower(), keyword:lower()) then
            addButton(tab, btn.title, btn.description, btn.url)
        end
    end
end

